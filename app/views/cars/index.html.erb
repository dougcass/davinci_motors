<%= link_to 'New Car', new_car_path %>

<h1>Listing Cars</h1>

<table>
  <thead>
  <tr>
    <th>Make</th>
    <th>Model</th>
    <th>Year</th>
    <th>Price</th>
    <th colspan="3"></th>
  </tr>
  </thead>

<tbody>
<% @cars.each do |car| %>
  <tr>
    <td><%= car.make %></td>
    <td><%= car.model %></td>
    <td><%= car.year %></td>
    <td><%= car.price %></td>
    <td><%= link_to 'Edit', edit_car_path(car) %></td>
  </tr>
<% end %>
</tbody>
</table>
